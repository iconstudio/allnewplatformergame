/// @description Set the key maps
keyboard_set_map(vk_space, vk_enter)
keyboard_set_map(ord(","), vk_enter)
keyboard_set_map(ord("X"), vk_enter)
keyboard_set_map(ord("."), vk_escape)
keyboard_set_map(ord("C"), vk_escape)

keyboard_set_map(ord("J"), vk_left)
keyboard_set_map(ord("L"), vk_right)
keyboard_set_map(ord("I"), vk_up)
keyboard_set_map(ord("K"), vk_down)
